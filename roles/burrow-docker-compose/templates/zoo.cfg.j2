dataDir=/data
dataLogDir=/datalog
tickTime=2000
initLimit=5
syncLimit=2
autopurge.snapRetainCount={{ zookeeper_autopurge_snap_count }}
autopurge.purgeInterval={{ zookeeper_autopurge_interval }}
maxClientCnxns=60
standaloneEnabled=true
admin.enableServer=true
server.1=localhost:2888:3888;{{ zookeeper_port }}
4lw.commands.whitelist=srvr, mntr
metricsProvider.className=org.apache.zookeeper.metrics.prometheus.PrometheusMetricsProvider
metricsProvider.httpPort={{ zookeeper_metrics_port }}